{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "K21-covid-rep-adf-19"
		},
		"demobircks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'demobircks'"
		},
		"ls_ablob_covidrepsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidrepsa'"
		},
		"ls_adls_covidrepdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidrepdl'"
		},
		"ls_db_covid_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_db_covid_cluster'"
		},
		"ls_sql_covid_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_covid_db'"
		},
		"demobircks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0502-114153-nuh1s0nq"
		},
		"is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/hbuddana/Azure_Data_Factory_COVID-19_Reporting/refs/heads/main/dataset/ds_ecdc_file_list.json"
		},
		"ls_adls_covidrepdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://k21covidrepadls19.dfs.core.windows.net/"
		},
		"ls_sql_covid_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "k21-covid-rep-srv-19.database.windows.net"
		},
		"ls_sql_covid_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "k21-covid-db-19"
		},
		"ls_sql_covid_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"httpdiabetesURL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/ammarasmro/mslearn-dp100/refs/heads/main/data/diabetes.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "K21-covid-rep-adf-19_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"demobircks_accessToken": {
						"value": "[parameters('demobircks_accessToken')]"
					},
					"ls_ablob_covidrepsa_connectionString": {
						"value": "[parameters('ls_ablob_covidrepsa_connectionString')]"
					},
					"ls_adls_covidrepdl_accountKey": {
						"value": "[parameters('ls_adls_covidrepdl_accountKey')]"
					},
					"ls_db_covid_cluster_accessToken": {
						"value": "[parameters('ls_db_covid_cluster_accessToken')]"
					},
					"ls_sql_covid_db_password": {
						"value": "[parameters('ls_sql_covid_db_password')]"
					},
					"demobircks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('demobircks_properties_typeProperties_existingClusterId')]"
					},
					"is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('is_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"ls_adls_covidrepdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidrepdl_properties_typeProperties_url')]"
					},
					"ls_sql_covid_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_covid_db_properties_typeProperties_server')]"
					},
					"ls_sql_covid_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_covid_db_properties_typeProperties_database')]"
					},
					"ls_sql_covid_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_covid_db_properties_typeProperties_userName')]"
					},
					"httpdiabetesURL_properties_typeProperties_url": {
						"value": "[parameters('httpdiabetesURL_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "K21-covid-rep-adf-19_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'K21-covid-rep-adf-19_ArmTemplate_0')]"
			]
		}
	]
}